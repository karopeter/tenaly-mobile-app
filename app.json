{
  "expo": {
    "name": "Tenaly",
    "slug": "tenaly-mobile",
    "description": "A mobile app for Tenaly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tenaly-splash.png",
    "platforms": [
      "android",
      "ios"
    ],
    "backgroundColor": "#FFFFFF",
    "scheme": "tenaly",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karopeter531.tenaly",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload ad images.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "tenaly"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "tenaly"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.karopeter531.tenaly",
      "permissions": [
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tenaly-splash.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "tenaly",
              "host": "wallet"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "edgeToEdgeEnabled": true,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tenaly-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3044a739-d08b-4ff0-a79e-2563f43c6829"
      }
    }
  }
}
