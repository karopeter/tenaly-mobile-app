{
  "expo": {
    "name": "Tenaly",
    "slug": "tenaly-mobile",
    "description": "A mobile app for Tenaly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tenaly-splash.png",
    "platforms": [
      "android",
      "ios"
    ],
    "backgroundColor": "#FFFFFF",
    "scheme": "tenaly",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tenaly-splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.karopeter531.tenaly",
      "edgeToEdgeEnabled": true,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tenaly-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3044a739-d08b-4ff0-a79e-2563f43c6829"
      }
    }
  }
}
