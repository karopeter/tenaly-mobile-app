{
  "expo": {
    "name": "Tenaly",
    "slug": "tenaly-mobile",
    "description": "A mobile app for Tenaly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tenalySplash.png",
    "platforms": ["android", "ios"],
    "backgroundColor": "#FFFFFF",
    "scheme": "tenaly",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karopeter531.tenaly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tenalySplash.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.karopeter531.tenaly"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tenalySplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_API_URL": "https://api.tenaly.com"
    }
  }
}




